<dl>
  {{#each modifiers}}
  <dt>
    No List<br />
    <small>
      <code>c-nav</code>
      <code>c-nav--no-list</code>
      {{#each this}}<code>c-nav--{{.}}</code>{{/each}}
    </small>
  </dt>
  <dd>
    <nav class="c-nav c-nav--no-list{{#each this}} c-nav--{{.}}{{/each}}">
    {{> @link-list links=../links }}
    </nav>
  </dd>
  {{/each}}
</dl>
