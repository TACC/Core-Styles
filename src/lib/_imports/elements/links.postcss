@import url("../components/c-button.selectors.postcss");
@import url("../tools/selectors.postcss");
@import url("../tools/x-link.postcss");

a:where(:not(:--c-button)) {
  @mixin link;
}
a:where(:not([href])) {
  @mixin link--disabled;
}
a:where(:not(:--c-button)):hover {
  @mixin link--hover;
}
a:where(:not(:--c-button)):active {
  @mixin link--active;
}
