@import url('../components/c-button.selectors.css');
@import url("../tools/selectors.css");
@import url('../tools/x-link.css');

a:not(:--c-button) {
  @mixin link;
}
a:not([href]) {
  @mixin link--disabled;
}
a:not(:--c-button):hover {
  @mixin link--hover;
}
a:not(:--c-button):active {
  @mixin link--active;
}
