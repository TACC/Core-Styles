<dl {{#if class }}class="{{ class }}"{{/if}}>
  {{#each data.key-value-pairs}}
    {{#if key}}
    <dt {{#if key-class}}class="{{ key-class }}"{{/if}}>
      {{ key }}
    </dt>
    {{/if}}
    {{#if value}}
    <dd {{#if value-class}}class="{{ value-class }}"{{/if}}>
      {{ value }}
    </dd>
    {{/if}}
  {{/each}}
</dl>
